{
  "name": "kpp-help-center",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:api": "npm run dev --workspace=@kpp/api",
    "dev:web": "npm run dev --workspace=@kpp/web",
    "db:generate": "npm run db:generate --workspace=@kpp/api",
    "db:push": "npm run db:push --workspace=@kpp/api",
    "db:studio": "npm run db:studio --workspace=@kpp/api"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
